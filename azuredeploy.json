{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location":{
        "type":"string",
        "defaultvalue":"[resourceGroup().location]"
        },
        "serverfarmName":{
            "type":"string",
            "defaultValue": "[concat('kaispe', uniqueString(resourceGroup().id))]"
        },
        "WebAppName":{
            "type":"string",
            "defaultValue": "[concat('kaispeWebApp', uniqueString(resourceGroup().id))]"
        }

      
    },
"resources":[
{
    "name": "[parameters('serverfarmName')]",
    "type": "Microsoft.Web/serverfarms",
    "apiVersion": "2018-02-01",
    "kind": "string",
    "location": "[parameters('location')]",
    
    "sku": {
      "name": "S1"
      
     
    }
  },

  {
    "name": "[parameters('webAppName')]",
    "type": "Microsoft.Web/sites",
    "apiVersion": "2018-02-01",
    
    "location": "[parameters('location')]",
    "tags": {},
    "properties": {
      "serverFarmId": "[resourceId('Microsoft.Web/serverfarms',parameters('serverfarmName'))]"

    
    },
    
    "dependsOn": [
        "[resourceId('Microsoft.Web/serverfarms',parameters('serverfarmName'))]"
              ]
    },
   
    
  {
    "name": "[concat(parameters('webAppName'))]",
    "type": "Microsoft.Web/sites/extensions",
    "apiVersion": "2018-02-01",
    
    "location": "[parameters('location')]",
    "tags": {},
    "properties": {
      "addOnPackages":[
          {
              "packageUri":"https://kspwebdeployment.blob.core.windows.net/webfile/AzureIOTPortal.zip?sp=r&st=2019-09-04T06:33:23Z&se=2019-09-05T14:33:23Z&spr=https&sv=2018-03-28&sig=Ffygo0ngVCuT8xrT5sZTcmuX%2BcZcHfNXh4dZ1mzH7fQ%3D&sr=b"
          }
      ]
    },
    
    "dependsOn": [
        "[resourceId('Microsoft.Web/sites',parameters('webAppName'))]"
              ]
    }

]
}